    "tu-azulverde": {
        "moreinfo": [
            "Me pierdo",
            "y caigo en picado,",
            "desnuda y sin nada,",
            "en el océano helado",
            "del azul de tu mirada,",
            "que me abriga,",
            "que me empaña,",
            "que me rompe,",
            "que me araña.",
            "",
            "Me escondo,",
            "y del mal me despojo,",
            "en la inmensidad verde",
            "del bosque de tus ojos.",
            "Donde sueño despierta,",
            "donde la salvia me cuenta",
            "que tú eres mi sino,",
            "mi ruta, mi camino."
        ]
    },
    "io": {
        "moreinfo": [
            "En el monótono esplendor",
